<p id="notice"><%= notice %></p>

<h1>End Of Days</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Hours in meetings</th>
      <th>Hours on expected tasks</th>
      <th>Hours on unexpected tasks</th>
      <th>Got done</th>
      <th>To do</th>
      <th>Concerns</th>
      <th>Learned</th>
      <th>Published</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @end_of_days.each do |end_of_day| %>
      <tr>
        <td><%= end_of_day.user_id %></td>
        <td><%= end_of_day.hours_in_meetings %></td>
        <td><%= end_of_day.hours_on_expected_tasks %></td>
        <td><%= end_of_day.hours_on_unexpected_tasks %></td>
        <td><%= end_of_day.got_done %></td>
        <td><%= end_of_day.to_do %></td>
        <td><%= end_of_day.concerns %></td>
        <td><%= end_of_day.learned %></td>
        <td><%= end_of_day.published %></td>
        <td><%= link_to 'Show', end_of_day %></td>
        <td><%= link_to 'Edit', edit_end_of_day_path(end_of_day) %></td>
        <td><%= link_to 'Destroy', end_of_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New End Of Day', new_end_of_day_path %>
